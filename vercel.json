{
  "rewrites": [
    { "source": "/index.html", "destination": "/login" }
  ],
  "redirects": [
    { "source": "/old-path", "destination": "/new-path", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Custom-Header", "value": "MyValue" }
      ]
    }
  ]
}
